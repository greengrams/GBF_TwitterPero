{
	"name": "GBF_Pero",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Get more item!",
	"background": {
		"scripts": ["background.js"]
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"browser_action": {
		"default_icon": "off.png",
		"default_title": "GBF_Pero",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"clipboardWrite"
	],
	"web_accessible_resources": [
		"on.png","off.png","alert.wav"
	]
}